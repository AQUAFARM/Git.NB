<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="@color/grey300"/>
            <stroke android:width="1px" android:color="@color/grey300" />
            <corners android:radius="150dp"/>
        </shape>
    </item>
    <item android:top="1px" android:bottom="1px" android:left="1px" android:right="1px">
        <shape>
            <solid android:color="@color/milk_white" />
            <stroke android:width="1px" android:color="@color/milk_white" />
            <corners android:radius="150dp"/>
        </shape>
    </item>
</layer-list>